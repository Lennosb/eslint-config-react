{
  "name": "@lennardvdberg/eslint-config-react-lennard",
  "version": "0.0.0-development",
  "description": "My ESLint configuration for React",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lennosb/eslint-config-react.git"
  },
  "author": "Lennard van den Berg <lennardvandenberg@hotmail.com>",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ]
  },
  "scripts": {
    "auditPackages": "yarn audit-ci -l -p yarn",
    "lint": "eslint --ext .ts,.tsx . --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "8.46.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "prettier": "3.0.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "audit-ci": "6.6.1",
    "semantic-release": "21.0.7"
  },
  "bugs": {
    "url": "https://github.com/Lennosb/eslint-config-react/issues"
  },
  "homepage": "https://github.com/Lennosb/eslint-config-react#readme",
  "keywords": [
    "eslint",
    "typescript",
    "prettier"
  ]
}
