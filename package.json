{
  "name": "@lennardvdberg/eslint-config-react-lennard",
  "version": "0.0.0-development",
  "description": "My ESLint configuration for React",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lennosb/eslint-config-react.git"
  },
  "author": "Lennard van den Berg <lennardvandenberg@hotmail.com>",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ]
  },
  "scripts": {
    "auditPackages": "yarn audit-ci -l -p yarn",
    "lint": "eslint --ext .ts,.tsx . --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "prettier": "2.5.1",
    "typescript": "4.6.2"
  },
  "devDependencies": {
    "audit-ci": "6.1.0",
    "semantic-release": "19.0.2"
  },
  "bugs": {
    "url": "https://github.com/Lennosb/eslint-config-react/issues"
  },
  "homepage": "https://github.com/Lennosb/eslint-config-react#readme",
  "keywords": [
    "eslint",
    "typescript",
    "prettier"
  ]
}
